<script>
var olandersCode = document.querySelector(".landers-code")
var twoDimension = olandersCode.querySelector(".two-dimension")
var odimensionZzc = document.querySelector(".dimension-zzc")
var osj = document.querySelector(".sj")
var maskedCode = document.querySelector(".masked-code")
var oa = document.querySelector(".lander").querySelectorAll("a")
var op = document.querySelector(".lander").querySelectorAll("p")
var ozzc = document.querySelector(".zzc")
var orefresh = ozzc.querySelector(".refresh")
var codeLanding = document.querySelector(".code-landing")
var accountLanding = document.querySelector(".account-landing")
var oError = document.querySelector(".Error")
var oshi = document.querySelector(".shi")
var oyan = document.querySelector(".yan")
var oname = document.getElementById("name")
var opassword = document.getElementById("password")

for(var i=0;i<op.length;i++){
	for(var i=0;i<oa.length;i++){
		op[0].onclick = function(){
			for(var i=0;i<oa.length;i++){
				oa[i].className = '';//先清空所有类名
			}
			olandersCode.style.display = "block"
			maskedCode.style.display = "none"
			oa[0].className = 'landing';//先清空所有类名
		}
		op[1].onclick = function(){
			for(var i=0;i<oa.length;i++){
				oa[i].className = '';//先清空所有类名
			}
			maskedCode.style.display = "block"
			olandersCode.style.display = "none"
			oa[1].className = 'landing';//先清空所有类名
		}
	}
}

function startMove(obj,attr,iTarget, fnEnd){		//简单的运动框架
	clearInterval(obj.timer);
	obj.timer=setInterval(function (){
		var cur=0;
		cur=parseInt(getComputedStyle(obj, false)[attr]);
		var speed=(iTarget-cur)/6;
		speed=speed>0?Math.ceil(speed):Math.floor(speed);
		obj.style[attr]=cur+speed+'px';
		if(cur === iTarget){
			clearInterval(obj.timer);
			if(fnEnd)fnEnd();
		}
	},20)
}

window.onload = function(){
	startMove(odimensionZzc,"left",25,function(){
		osj.style.display = "block"

	})
	function gb(){
			osj.style.display = "none"
			startMove(odimensionZzc,"left",90,function(){
		})
	}
	setTimeout(gb,5000)
}
twoDimension.onmouseenter = function(){
	startMove(odimensionZzc,"left",25,function(){
		osj.style.display = "block";
	})
}
twoDimension.onmouseleave = function(){
	osj.style.display = "none";
	startMove(odimensionZzc,"left",90)
}

timer = setInterval(function (){
	ozzc.style.display = "block"
	twoDimension.onmouseenter = function(){
		return false
	}
},10000)
orefresh.onclick = function(){
	ozzc.style.display = "none"
	twoDimension.onmouseenter = function(){
		startMove(odimensionZzc,"left",25,function(){
			osj.style.display = "block"
		})
	}
	twoDimension.onmouseleave = function(){
		osj.style.display = "none"
		startMove(odimensionZzc,"left",90)
	}
}


function checkfrom(){
	var re = /^[a-zA-Z]\w{3,15}$/;
	var reg = /^1\d{10}$/;
	var regs = /^(\w-*\.*)+@(\w-?)+(\.\w{2,})+$/
	if(re.test(oname.value)||reg.test(oname.value)||regs.test(oname.value)){
		if(opassword.value == ""){
			oError.style.visibility = "visible";
			oshi.innerHTML = "请输入密码";
			return false;
		}
	}else if(oname.value == ""&&opassword.value == ""){
		oError.style.visibility = "visible";
		oshi.innerHTML = "请输入账户名和密码";
		return false
	}else{
		oError.style.visibility = "visible";
		oshi.innerHTML = "请输入正确的用户名";
		return false;
	}

	return true;
}
</script>
