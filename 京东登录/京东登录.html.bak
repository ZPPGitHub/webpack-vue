<link type = "text/css" rel = "stylesheet" href = "font/iconfont.css"/>
<style>
*{margin:0;padding:0}
body{font:12px Arial,Verdana,"\5b8b\4f53"}
ul{list-style:none}
a{color:#999;text-decoration:none;}
#passport{position:relative}
header{width:990px;margin:0 auto}
.logo{width:300px;height:60px;position:relative;margin:10px 0}
.logo b{display:block;width:110px;height:40px;background:url("imges/截图01.png") no-repeat -188px -14px;
position:absolute;bottom:0;right:5px}
header .quest:last-of-type{position:relative;float:right;bottom:30px;font-size:13px}
.quest span{display:inline-block;width:18px;height:14px;background:url("imges/截图02.png") no-repeat -2px -2px;
vertical-align:middle;margin:-1px 5px 0 5px}
.quest:hover{text-decoration:underline;color:#e4393c}
section{clear:both}
.network{width:100%;background:#ffebd7;padding:10px 0;font-size:13px;color:#999;display:flex;justify-content:center}
.network .icon{font-size:25px;line-height:18px}
.policy{color:#000;}
.policy:hover{text-decoration:underline;}
.wrap{height:475px;background:#e52645}
.wall{width:990px;height:475px;margin:0 auto;background:url("imges/截图03.png") no-repeat;position:relative}
.forms{width:346px;background:#fff;position:absolute;right:0;top:10px}
.lander{display:flex;height:54px;border-bottom:1px #ebebeb solid;font-size:18px;text-align:center;line-height:54px}
.lander p{width:173px;}
.lander p a{padding:0 49px;}
.lander p:first-of-type a{border-right:1px #ebebeb solid;}
.landing{color:#e4393c;font-weight:bold}
.lander p a:hover{color:#e4393c;font-weight:bold}
.landers{position:relative;border-bottom:1px #ebebeb solid;}
.landers-code{height:284px;}
.two-dimension{display:flex;position:relative;top:18px;height:165px}
.dimension-zzc{width:173px;height:165px;position:absolute;left:90px}
.zzc{width:170px;height:163px;background:rgba(0,0,0,.7);position:absolute;top:1px;left:1px;text-align:center;display:none}
.zzc p{color:#fff;margin-top:50px}
.refresh{display:inline-block;width:80px;height:30px;border:0;outline:0;background:#e4393c;color:#fff;font-size:16px;margin-top:20px;cursor:pointer;line-height:30px}
.two-dimension .sj{position:absolute;width:152px;height:172px;background:url("imges/截图05.png") no-repeat 7px -2px;right:0;display:none}
.account-code{width:100%;height:96px;position:absolute;bottom:0;font-size:13px;line-height:20px}
.account-code p{text-align:center;margin-top:15px}
.account-code p a{color:#e4393c}
.account-icon{display:flex;justify-content:center;color:#919191;}
.account-icon .icon{font-size:25px;color:#cdcdcd;vertical-align:sub;margin-left:15px}
.masked-code{position:relative;padding-bottom:20px;display:none}
.Error{width:304px;min-height:30px;background:rgba(200,0,0,.2);margin:5px auto;border:1px rgba(130,0,0,.3) solid;visibility:hidden;color:#e4393c}
.masked-code ul{margin-top:5px;}
.masked-code ul li{width:304px;height:38px;margin:0 auto;position:relative;border:1px #b3b3b3 solid;
margin-bottom:20px;line-height:38px}
.masked-code label{display:inline-block;width:38px;height:38px;background:#e0e0e0;position:absolute;border-right:1px  #b3b3b3 solid;text-align:center;color:#b3b3b3;}
.masked-code .name{width:254px;position:absolute;overflow:hidden;font-size:15px;right:0;top:12px;border:0;outline:0}
.forget{width:304px;text-align:right;margin:0 auto;font-size:12px;margin-bottom:20px}
.forget a{color:#000}
.forget a:hover{text-decoration:underline;color:#e52645}
.masked-code .yan{width:306px;height:34px;text-align:center;background:#e72341;border:0;outline:0;font-size:23px;
margin:0 0 10px 20px;color:#fff;cursor:pointer;line-height:34px}
.other{padding:0 20px;display:flex;position:relative;line-height:50px;background:#fcfcfc}
.other .Tencent{display:flex;font-size:12px}
.Tencent .icon{font-size:17px;vertical-align:sub;}
.Tencent a{color:#666}
.Tencent a span:hover{text-decoration:underline;color:#e52645}
.Tencent p{color:#b3b3b3;margin:0 10px}
.registry{position:absolute;right:20px;font-size:14px}
.registry a{color:#e72341}
.registry .icon{font-size:20px;vertical-align:sub;margin-right:3px}
.registry span:hover{text-decoration:underline}
footer{position:relative;text-align:center;color:#666}
.external ul{display:flex;justify-content:center;margin-top:22px;margin-bottom:15px}
.external ul li{margin:0 7px}
.external ul a{color:#666}
.external ul a:hover{text-decoration:underline;;color:#e52645}
</style>
</head>
<body>
